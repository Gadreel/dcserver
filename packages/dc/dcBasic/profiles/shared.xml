<Config>
	<Packages.Template Id="main-packages">
		<Package Name="dc/dcHub" />
		<Package Name="dc/dcApp" />
	</Packages.Template>

  <Service.Template Id="main-web" Inherit="dc-web-full">
      <Settings DefaultSecurePort="8443">
          <HttpListener Port="8443" Secure="true" />
          <HttpListener Port="8080" Secure="false" />
      </Settings>
  </Service.Template>

	<Service.Template Id="main-db" Inherit="dc-database-rocks" />

	<Service.Template Id="main-sessions" Inherit="dc-sessions" />

	<Service.Template Id="main-core" Inherit="dc-core-db" />

	<Service.Template Id="main-cms" Inherit="dcm-cms" />

	<Service.Template Id="main-store" Inherit="dcm-store-simple" />
</Config>
