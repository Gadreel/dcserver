<dcs.Script>
	<dcs.CatalogSettings Id="Interchange-Aws" Result="AwsConfig" />

	<dcs.Var Name="awsStore" Type="dcmAwsCDNStore">
		<Connect AwsBucket="$AwsConfig.@CDNBucket" AwsAccount="default" RootFolder="/{$_Node.Deployment}/{$_Tenant.Alias}/files" />
		<FolderListing Path="$Data.Path" Handle="Listing" />
	</dcs.Var>

	<dcs.Exit Scope="Task" Result="$Listing" />
</dcs.Script>
